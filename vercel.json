{
  "version": 2,
  "name": "grammar-parser-api",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key"
  },
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  }
}